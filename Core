        if (args[0] === "on") {
          if (antilinkData.includes(groupId)) return reply('Bereits aktiviert');

          antilinkData.push(groupId);
          saveAntilinkData();

          reply('Antilink Aktiviert!');

          var groupe = await Phoenix.groupMetadata(from);
          //  var members = groupe['participants'];
