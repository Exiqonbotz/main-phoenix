        const malScraper = require('mal-scraper')
        reply(mess.waiting);
        const anime = await malScraper.getInfoFromName(text).catch(() => null)
        if (!anime) return m.reply(`${p}Could not find your scarch`)
        let animetxt = `
  ğŸ€ *Title: ${anime.title}*
  ğŸ‹ *Type: ${anime.type}*
  ğŸ *Premiered on: ${anime.premiered}*
